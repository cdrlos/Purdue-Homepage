export MOSH_LOADPATH="$PWD/.akku/lib"
export PLTCOLLECTS=":$PWD/.akku/lib"
export SAGITTARIUS_LOADPATH="$PWD/.akku/lib"
export VICARE_SOURCE_PATH="$PWD/.akku/lib"
export YPSILON_SITELIB="$PWD/.akku/lib"
export LARCENY_LIBPATH="$PWD/.akku/lib"
export PATH=$PWD/.akku/bin:$PATH
